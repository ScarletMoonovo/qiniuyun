syntax = "v1"

info(
    title: "基建模块"
    desc: "基建模块"
)

type (
    UploadTokenResponse {
        Token string `json:"token"`
        Key string `json:"key"`
    }
)


@server(
    prefix: api
    middleware: Auth
)

service api {
    @handler uploadToken      //生成客户端上传凭证
    post /upload/token returns (UploadTokenResponse)
}